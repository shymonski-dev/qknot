{
  "name": "react-example",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": "22.x"
  },
  "type": "module",
  "scripts": {
    "predev": "node ./scripts/check-node-version.mjs dev",
    "dev": "vite --port=3000 --host=0.0.0.0",
    "predev:standalone": "node ./scripts/check-node-version.mjs dev:standalone",
    "dev:standalone": "node ./scripts/dev-standalone.mjs",
    "prebuild": "node ./scripts/check-node-version.mjs build",
    "build": "node ./scripts/build.mjs",
    "preview": "vite preview",
    "clean": "rm -rf dist",
    "presetup:backend": "node ./scripts/check-node-version.mjs setup:backend",
    "setup:backend": "node ./scripts/setup-backend.mjs",
    "prelint": "node ./scripts/check-node-version.mjs lint",
    "lint": "tsc --noEmit",
    "pretest": "node ./scripts/check-node-version.mjs test",
    "test": "vitest run",
    "pretest:backend": "node ./scripts/check-node-version.mjs test:backend",
    "test:backend": "node ./scripts/run-backend-tests.mjs",
    "test:all": "npm run test && npm run test:backend"
  },
  "dependencies": {
    "@google/genai": "^1.29.0",
    "@tailwindcss/vite": "^4.1.14",
    "@vitejs/plugin-react": "^5.0.4",
    "better-sqlite3": "^12.4.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "lucide-react": "^0.546.0",
    "motion": "^12.23.24",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^3.7.0",
    "tailwind-merge": "^3.5.0",
    "vite": "^6.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^4.17.21",
    "@types/node": "^22.14.0",
    "autoprefixer": "^10.4.21",
    "happy-dom": "^20.7.0",
    "jsdom": "^26.1.0",
    "tailwindcss": "^4.1.14",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^3.2.4"
  }
}
